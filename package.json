{
  "name": "khusamov-backup",
  "version": "0.0.7",
  "description": "Создание резервных копий git-репозиторий",
  "author": "Khusamov Sukhrob",
  "license": "MIT",
  "type": "module",
  "main": "src/index.ts",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "bin": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir ./dist --target node --packages external",
    "build:watch": "bun build src/index.ts --outdir ./dist --target node --packages external --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "bun run build",
    "json2ts": "json2ts --input src/interfaces/github/schemas --output src/interfaces/github"
  },
  "keywords": [
    "github",
    "tools",
    "utilites",
    "backup"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/khusamov/backup.git"
  },
  "bugs": {
    "url": "https://github.com/khusamov/backup/issues",
    "email": "khusamov@yandex.ru"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "figlet": "^1.8.0",
    "octokit": "^4.1.2"
  },
  "devDependencies": {
    "@types/bun": "^1.2.3",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.13.5",
    "json-schema-to-typescript": "^15.0.4",
    "typescript": "^5.7.3"
  }
}
